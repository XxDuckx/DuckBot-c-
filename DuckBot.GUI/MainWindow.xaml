<Window x:Class="DuckBot.GUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:v="clr-namespace:DuckBot.GUI.Views"
        Title="DuckBot" Height="720" Width="1100"
        WindowStartupLocation="CenterScreen">
    <Grid Background="{StaticResource ApplicationBackgroundBrush}">
        <TabControl x:Name="Tabs">
            <TabItem Header="My Bots">
                <v:MyBotsView/>
            </TabItem>
            <TabItem Header="Logs">
                <v:LogsView/>
            </TabItem>
            <TabItem Header="Script Builder">
                <v:ScriptBuilderView/>
            </TabItem>
            <TabItem Header="Live Runner">
                <v:LiveRunnerView/>
            </TabItem>
            <TabItem Header="Settings">
                <v:SettingsView/>
            </TabItem>
            <TabItem Header="Updates">
                <v:UpdatesView/>
            </TabItem>
        </TabControl>
    </Grid>
</Window>